<?xml version='1.0' encoding='utf-8'?>
<widget id="be.bozga.immo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>My Immo Invest</name>
  <description>
    Vous souhaitez calculer ce que vous couteront les frais d'acquisition lors de l'achat de votre bien immobilier ou
    votre terrain ? Utilisez notre simulateur de calcul et obtenez une estimation des frais liés à l'acquisition et à
    l'ouverture d'un crédit.
  </description>
  <author email="bozga.andrei@gmail.com" href="www.bozga.be">
    Andrei Bozga
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <preference name="android-minSdkVersion" value="20"/>
    <preference name="android-targetSdkVersion" value="25"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>
